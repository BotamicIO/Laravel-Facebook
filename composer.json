{
    "name": "artisanry/facebook",
    "description": ":package_description",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Facebook",
        "Laravel Facebook"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8",
        "league/oauth2-facebook": "^2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Facebook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Facebook\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Facebook\\FacebookServiceProvider"
            ],
            "aliases": {
                "Facebook": "Artisanry\\Facebook\\Facades\\Facebook"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
